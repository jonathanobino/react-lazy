{
  "name": "lazy-react",
  "version": "3.1.1",
  "description": "components to lazy load iframes, images and images as backgrounds",
  "main": "./dist/index.js",
  "scripts": {
    "build:release": "webpack",
    "build:demo": "webpack --config webpack.config.demo.js",
    "build": "npm run build:release && npm run build:demo",
    "lint": "eslint src/*",
    "lint:fix": "eslint --fix src/*",
    "format": "prettier --write  --single-quote --no-semi ./src/*.tsx",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "src/*.js": [
      "prettier --write --single-quote --no-semi",
      "git add"
    ]
  },
  "keywords": [
    "react",
    "lazy-load"
  ],
  "author": "Jonathan Obino",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/jonathanobino/react-lazy"
  },
  "devDependencies": {
    "@testing-library/react": "^12.1.4",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "eslint": "^7.10.0",
    "eslint-plugin-react": "^7.21.3",
    "husky": "^4.3.0",
    "jest": "^27.5.1",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "ts-loader": "^9.2.7",
    "typescript": "^4.6.2",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "peerDependencies": {
    "react-dom": "^17.0.2"
  }
}
